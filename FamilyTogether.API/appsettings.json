{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Supabase": "Host=db.your-project-id.supabase.co;Port=5432;Database=postgres;Username=postgres;Password=your_password;SSL Mode=Require;Trust Server Certificate=true"
  },
  "Supabase": {
    "Url": "https://your-project-id.supabase.co",
    "PublishableKey": "your_supabase_publishable_key_here",
    "SecretKey": "your_supabase_secret_key_here",
    "JwtKeyId": "your_jwt_key_id_here",
    "JwtDiscoveryUrl": "https://your-project-id.supabase.co/auth/v1/.well-known/jwks.json"
  },
  "Jwt": {
    "Issuer": "https://your-project-id.supabase.co/auth/v1",
    "Audience": "authenticated",
    "ValidateIssuerSigningKey": true
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:8000",
      "http://localhost:3000",
      "http://localhost:5173"
    ]
  }
}
