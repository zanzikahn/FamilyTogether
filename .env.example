# FamilyTogether API - Environment Variables
# Copy this to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here
SUPABASE_SECRET_KEY=your_supabase_secret_key_here
SUPABASE_JWT_KEY_ID=your_jwt_key_id_here

# Database Connection
DATABASE_URL=postgresql://postgres:your_password@db.your-project-id.supabase.co:5432/postgres

# JWT Settings
JWT_DISCOVERY_URL=https://your-project-id.supabase.co/auth/v1/.well-known/jwks.json

# API Settings
ASPNETCORE_ENVIRONMENT=Development
API_PORT=5001

# CORS Settings (update with your SPA URL when deployed)
CORS_ALLOWED_ORIGINS=http://localhost:8000,http://localhost:3000

# Logging
LOG_LEVEL=Information
